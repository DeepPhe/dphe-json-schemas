{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "$defs" : {
    "Mention" : {
      "type" : "object",
      "properties" : {
        "begin" : {
          "type" : "integer"
        },
        "classUri" : {
          "type" : "string"
        },
        "confidence" : {
          "type" : "integer"
        },
        "end" : {
          "type" : "integer"
        },
        "historic" : {
          "type" : "boolean"
        },
        "id" : {
          "type" : "string"
        },
        "negated" : {
          "type" : "boolean"
        },
        "uncertain" : {
          "type" : "boolean"
        }
      }
    },
    "MentionCoref" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "idChain" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        }
      }
    },
    "MentionRelation" : {
      "type" : "object",
      "properties" : {
        "sourceId" : {
          "type" : "string"
        },
        "targetId" : {
          "type" : "string"
        },
        "type" : {
          "type" : "string"
        }
      }
    },
    "NeoplasmAttribute" : {
      "type" : "object",
      "properties" : {
        "classUri" : {
          "type" : "string"
        },
        "confidence" : {
          "type" : "integer"
        },
        "confidenceFeatures" : {
          "type" : "array",
          "items" : {
            "type" : "integer"
          }
        },
        "directEvidence" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "id" : {
          "type" : "string"
        },
        "indirectEvidence" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "name" : {
          "type" : "string"
        },
        "notEvidence" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "value" : {
          "type" : "string"
        }
      }
    },
    "NeoplasmSummary" : {
      "type" : "object",
      "properties" : {
        "attributes" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/NeoplasmAttribute"
          }
        },
        "classUri" : {
          "type" : "string"
        },
        "er" : {
          "type" : "string"
        },
        "her2" : {
          "type" : "string"
        },
        "id" : {
          "type" : "string"
        },
        "pathologic_m" : {
          "type" : "string"
        },
        "pathologic_n" : {
          "type" : "string"
        },
        "pathologic_t" : {
          "type" : "string"
        },
        "pr" : {
          "type" : "string"
        },
        "subSummaries" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/NeoplasmSummary"
          }
        }
      }
    },
    "NewBiomarkerSummary" : {
      "type" : "object",
      "properties" : {
        "patientId" : {
          "type" : "string"
        },
        "relationPrettyName" : {
          "type" : "string"
        },
        "tumorFactRelation" : {
          "type" : "string"
        },
        "valueText" : {
          "type" : "string"
        }
      }
    },
    "NewPatientDiagnosis" : {
      "type" : "object",
      "properties" : {
        "diagnosis" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "diagnosisGroups" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "patientId" : {
          "type" : "string"
        }
      }
    },
    "Note" : {
      "type" : "object",
      "properties" : {
        "corefs" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/MentionCoref"
          }
        },
        "date" : {
          "type" : "string"
        },
        "episode" : {
          "type" : "string"
        },
        "id" : {
          "type" : "string"
        },
        "mentions" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "name" : {
          "type" : "string"
        },
        "relations" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/MentionRelation"
          }
        },
        "sections" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Section"
          }
        },
        "text" : {
          "type" : "string"
        },
        "type" : {
          "type" : "string"
        }
      }
    },
    "Patient" : {
      "type" : "object",
      "properties" : {
        "biomarkers" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/NewBiomarkerSummary"
          }
        },
        "birth" : {
          "type" : "string"
        },
        "death" : {
          "type" : "string"
        },
        "diagnoses" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/NewPatientDiagnosis"
          }
        },
        "gender" : {
          "type" : "string"
        },
        "id" : {
          "type" : "string"
        },
        "name" : {
          "type" : "string"
        },
        "noteHash" : {
          "type" : "string"
        },
        "notes" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Note"
          }
        }
      }
    },
    "Section" : {
      "type" : "object",
      "properties" : {
        "begin" : {
          "type" : "integer"
        },
        "end" : {
          "type" : "integer"
        },
        "id" : {
          "type" : "string"
        },
        "type" : {
          "type" : "string"
        }
      }
    }
  },
  "type" : "object",
  "properties" : {
    "id" : {
      "type" : "string"
    },
    "neoplasms" : {
      "type" : "array",
      "items" : {
        "$ref" : "#/$defs/NeoplasmSummary"
      }
    },
    "patient" : {
      "$ref" : "#/$defs/Patient"
    }
  }
}