{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "$defs" : {
    "Map(String,List(String))" : {
      "type" : "object"
    },
    "Mention" : {
      "type" : "object",
      "properties" : {
        "begin" : {
          "type" : "integer"
        },
        "classUri" : {
          "type" : "string"
        },
        "confidence" : {
          "type" : "integer"
        },
        "end" : {
          "type" : "integer"
        },
        "historic" : {
          "type" : "boolean"
        },
        "id" : {
          "type" : "string"
        },
        "negated" : {
          "type" : "boolean"
        },
        "uncertain" : {
          "type" : "boolean"
        }
      }
    },
    "NeoplasmAttribute" : {
      "type" : "object",
      "properties" : {
        "classUri" : {
          "type" : "string"
        },
        "confidence" : {
          "type" : "integer"
        },
        "confidenceFeatures" : {
          "type" : "array",
          "items" : {
            "type" : "integer"
          }
        },
        "directEvidence" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "id" : {
          "type" : "string"
        },
        "indirectEvidence" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "name" : {
          "type" : "string"
        },
        "notEvidence" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Mention"
          }
        },
        "value" : {
          "type" : "string"
        }
      }
    }
  },
  "type" : "object",
  "properties" : {
    "attributes" : {
      "type" : "array",
      "items" : {
        "$ref" : "#/$defs/NeoplasmAttribute"
      }
    },
    "classUri" : {
      "type" : "string"
    },
    "factIds" : {
      "type" : "array",
      "items" : {
        "type" : "string"
      }
    },
    "id" : {
      "type" : "string"
    },
    "relatedFactIds" : {
      "$ref" : "#/$defs/Map(String,List(String))"
    }
  }
}